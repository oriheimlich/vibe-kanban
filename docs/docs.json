          "workspaces/interface",
          "workspaces/command-bar",
          "workspaces/multi-repo-sessions",
          "workspaces/preview",
          "workspaces/changes",
          "workspaces/git-operations"
        ]
      },
      {
        "group": "Cloud",
        "pages": [
          "cloud/index",
          "cloud/getting-started",
          "cloud/migration",
          "cloud/authentication",
